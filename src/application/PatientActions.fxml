      <TextField fx:id="pid" layoutX="271.0" layoutY="84.0" promptText="ID Number" />
      <Button fx:id="familyHist" layoutX="301.0" layoutY="157.0" mnemonicParsing="false" onAction="#actionSeeFamilyHist" text="Family history" />
      <TableView fx:id="tableF" layoutX="246.0" layoutY="209.0" prefHeight="200.0" prefWidth="200.0" />
      <Button fx:id="exit" layoutX="597.0" layoutY="384.0" mnemonicParsing="false" onAction="#actionExit" text="Exit" />
      <Button fx:id="back" layoutX="64.0" layoutY="384.0" mnemonicParsing="false" onAction="#actionBack" text="Back" />
   </children>
</AnchorPane>
