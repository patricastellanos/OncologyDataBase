<?xml version="1.0" encoding="UTF-8"?>

<?import com.gluonhq.charm.glisten.control.TextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LogInDoctorController">
   <children>
      <AnchorPane prefHeight="446.0" prefWidth="671.0" style="-fx-background-color: pink;">
         <children>
            <ImageView fitHeight="454.0" fitWidth="687.0" layoutX="-1.0" layoutY="-3.0" opacity="0.19">
               <image>
                  <Image url="@imagenLoginRegister.jpg" />
               </image>
            </ImageView>
            <PasswordField fx:id="passText" layoutX="232.0" layoutY="229.0" onAction="#actionPassword" prefHeight="26.0" prefWidth="186.0" promptText="Password" />
            <TextField fx:id="userText" layoutX="231.0" layoutY="187.0" prefHeight="26.0" prefWidth="186.0" promptText="Username" />
            <Button fx:id="loginButton1" layoutX="252.0" layoutY="295.0" mnemonicParsing="false" onAction="#actionLogin" text="Log in" />
            <Button fx:id="changePassButton" layoutX="537.0" layoutY="47.0" mnemonicParsing="false" onAction="#actionChangePass" text="Change password" />
            <Button fx:id="removeUserButton" layoutX="563.0" layoutY="83.0" mnemonicParsing="false" onAction="#actionRemoveUser" text="Remove user" />
         </children>
      </AnchorPane>
      <Button fx:id="backButton" layoutX="526.0" layoutY="375.0" mnemonicParsing="false" onAction="#actionBack" text="Back" />
      <Button fx:id="exitButton" layoutX="78.0" layoutY="375.0" mnemonicParsing="false" onAction="#actionExit" text="Exit" />
   </children>
</AnchorPane>
